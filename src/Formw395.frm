Private Sub Command5_Click()
sql1 = ""

If Check2(0).value = 1 Then
t1 = Format(DTPicker1.value, "yyyy-mm-dd")
t2 = Format(DTPicker2.value, "yyyy-mm-dd")
sql1 = sql1 + "日期 between cast('" & t1 & "' as datetime) and cast('" & t2 & "' as datetime) and "
End If

If Check2(1).value = 1 Then
sql1 = sql1 + "库类 like '%'+'" & DataCombo1.Text & "'+'%' and "
End If


If Check2(7).value = 1 Then
sql1 = sql1 + "材料名称 like '%'+'" & DataCombo2.Text & "'+'%' and "
End If


If sql1 = "" Then
MsgBox ("请选择查询条件")
Exit Sub
End If
sql1 = Left$(Trim(sql1), Len(Trim(sql1)) - 4)
Adodc1.RecordSource = "SELECT 日期,供应单位,材料名称,材料规格,材料单位,颜色,批次,单价,上月结存数量,上月结存金额,本月入库数量,本月入库金额,本月出库数量,本月出库金额,本月结存数量,本月结存金额,损耗数量,损耗金额 FROM clbbzcw  where (" + sql1 + ")  ORDER BY 材料名称,日期"
Adodc1.Refresh

End Sub


