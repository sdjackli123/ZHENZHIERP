Private Sub Command6_Click()
sql1 = ""

If Check2(0).value = 1 Then
sql1 = sql1 + "班次 like '%'+'" & Combo1.Text & "'+'%' and "
End If

If Check2(2).value = 1 Then
sql1 = sql1 + "单号 like '%'+'" & DataCombo2.Text & "'+'%' and "
End If

If Check2(3).value = 1 Then
sql1 = sql1 + "机台='" & DataCombo1.Text & "' and "
End If

If Check2(4).value = 1 Then
sql1 = sql1 + "日期 between cast('" & DTPicker2.value & "' as datetime) and cast('" & DTPicker3.value & "' as datetime) and "
End If

If Check2(7).value = 1 Then
sql1 = sql1 + "缸号='" & DataCombo3.Text & "' and "
End If

If sql1 = "" Then
MsgBox ("请选择查询条件")
Exit Sub
End If
sql1 = Left$(Trim(sql1), Len(Trim(sql1)) - 4)
Adodc1.RecordSource = "select * FROM v_rsclbb_sfmg where (" + sql1 + ") order by 日期"
Adodc1.Refresh
Adodc7.RecordSource = "select round(sum(匹数),1) as 合计匹数,round(sum(重量),2) as 合计重量 FROM v_rsclbb_sfmg where (" + sql1 + ")"
Adodc7.Refresh
End Sub

