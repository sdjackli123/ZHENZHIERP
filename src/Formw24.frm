Adodc5.RecordSource = "SELECT 简称 from khzl group by 简称"
Adodc5.Refresh
End If
End Sub

Private Sub Command1_Click()
If Combo1.Text = "应付账款" Then
Adodc1.RecordSource = "SELECT * FROM JGZCXF WHERE 结转日期 BETWEEN cast('" & DTPicker1.value & "' as datetime) AND cast('" & DTPicker2.value & "' as datetime) ORDER BY 结转日期,客户"
Adodc1.Refresh
