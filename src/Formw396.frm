Private Sub Command2_Click()
If DataCombo2.Text = "" Then
Adodc1.RecordSource = "SELECT * FROM mppcbb where 日期 between cast('" & DTPicker1.value & "' as datetime) AND cast('" & DTPicker2.value & "' as datetime) ORDER BY 品名,日期"
Adodc1.Refresh
Else
Adodc1.RecordSource = "SELECT * FROM mppcbb  where 日期 between cast('" & DTPicker1.value & "' as datetime) AND cast('" & DTPicker2.value & "' as datetime) and 品名='" & DataCombo2.Text & "' ORDER BY 品名,日期"
Adodc1.Refresh
End If
End Sub

Private Sub Command3_Click()
