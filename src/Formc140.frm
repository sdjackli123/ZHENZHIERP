Adodc1.Recordset.Move rs - 1
Text2.Text = Adodc1.Recordset.Fields(6)
End Sub

Private Sub Text1_Change()
On Error Resume Next
Adodc1.ConnectionString = "Provider=SQLOLEDB.1;Password=fjrj;Persist Security Info=True;User ID=sa;Initial Catalog=zzpr;Data Source=192.168.1.254"
Adodc1.RecordSource = "select 客户名称,品名,色别,重量,匹数,备注,ip from kpd  WHERE 锅号='" & Text1.Text & "' order by ip"
Adodc1.Refresh

Adodc2.ConnectionString = "Provider=SQLOLEDB.1;Password=fjrj;Persist Security Info=True;User ID=sa;Initial Catalog=zzpr;Data Source=192.168.1.254"
Adodc2.RecordSource = "select distinct 配料编号 from bgxx  WHERE 并缸锅号='" & Text1.Text & "'"
Adodc2.Refresh
If Not Adodc2.Recordset.EOF Then
Text3 = Adodc2.Recordset.Fields(0)
Adodc3.ConnectionString = "Provider=SQLOLEDB.1;Password=fjrj;Persist Security Info=True;User ID=sa;Initial Catalog=zzpr;Data Source=192.168.1.254"
Adodc3.RecordSource = "select 并缸锅号 from bgxx  WHERE 配料编号='" & Adodc2.Recordset.Fields(0) & "'"
Adodc3.Refresh
End If

