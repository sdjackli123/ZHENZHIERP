Private Sub Command1_Click()
On Error Resume Next
If DataCombo1.Text = "" Then
Adodc2.RecordSource = "select * from MPCKGL3 where 重量>0 and 客户名称 is not null order by 客户名称,布类"
Adodc2.Refresh
Else
Adodc2.RecordSource = "select * from MPCKGL3 where 重量>0 and  客户名称='" & DataCombo1.Text & "' order by 客户名称,布类"
Adodc2.Refresh
End If

If VSFlexGrid1.Rows > 1 Then
For i = 1 To VSFlexGrid1.Rows - 1
VSFlexGrid1.RowHeight(i) = 500
Next
End If
End Sub

Private Sub Command2_Click()
Call OutadodcToExcel(VSFlexGrid1, 5, DataCombo1.Text + "毛坯库存")
End Sub

Private Sub Command3_Click()
Unload Me
End Sub

Private Sub Command4_Click()
If MsgBox("确定结转吗？结转到的日期为" + Trim(DTPicker3.value), vbYesNo) = vbNo Then Exit Sub
    Set g_Cmd = New Command
    g_Con = "Provider=SQLOLEDB.1;Password=fjrj;Persist Security Info=True;User ID=sa;Initial Catalog=zzpr;Data Source=192.168.1.254"
    g_Cmd.ActiveConnection = g_Con          ' 连接到数据库
    g_Cmd.CommandType = adCmdStoredProc     ' 表示cmd的类型为存储过程
    g_Cmd.CommandText = "MPKCJZ('" & DTPicker3.value & "')"       ' 表示调用哪个存储过程
    g_Cmd.Execute           ' 执行存储过程
    g_Cmd.Cancel
MsgBox ("结转成功！")
End Sub

